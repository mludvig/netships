#!/bin/sh

DAT=`date`
echo -n $DAT|cut -d\  -f3 > /tmp/archdate.0
echo -n $DAT|cut -d\  -f2 >> /tmp/archdate.0
echo -n $DAT|cut -d\  -f7 >> /tmp/archdate.0
echo -n - >> /tmp/archdate.0
echo -n $DAT|cut -d\  -f4|cut -d: -f1 >> /tmp/archdate.0
echo -n $DAT|cut -d\  -f4|cut -d: -f2 >> /tmp/archdate.0
echo -n $DAT|cut -d\  -f4|cut -d: -f3 >> /tmp/archdate.0

for i in `cat /tmp/archdate.0`; do
	echo -n $i
done
rm -f /tmp/archdate.0
